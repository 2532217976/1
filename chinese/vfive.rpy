translate chinese take_cover1_db982c70:

    # fr "Ooooff!"
    fr "Ooooff!"

# game/vfive.rpy:1014
translate chinese take_cover1_981de6a0:

    # m "Faye?"
    m "菲伊?"

# game/vfive.rpy:1017
