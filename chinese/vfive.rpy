    rs "Noooo!"

# game/vfive.rpy:960
translate chinese take_cover1_c47183c2:

    # ah "I got you Rachel! You're out."
    ah "我抓到你了,瑞秋!你出局了."

# game/vfive.rpy:961
translate chinese take_cover1_3b31f515:

    # rs "No fair."
    rs "不公平."

# game/vfive.rpy:965
