translate chinese drama_twins_249137f2:

    # mal "Hello Headmaster."
    mal "你好,校长."

# game/events.rpy:4735
translate chinese drama_twins_ef560e4d:

    # m "Who are you two supposed to be?"
    m "你们两个是谁?"

# game/events.rpy:4737
translate chinese drama_twins_bd3e4aa8:

    # mw "I'm Herdone the goddess of pleasure. Daughter of Cupid and Psyche."
    mw "我是快乐女神赫多.丘比特和普赛克的女儿."

# game/events.rpy:4739
