translate chinese drama_alice_45a241ce:

    # m "Having some trouble with Alice's costume?"
    m "爱丽丝的戏服有问题吗?"

# game/events.rpy:4718
translate chinese drama_alice_5fcd58a6:

    # al "Headmaster!"
    al "校长!"

# game/events.rpy:4720
translate chinese drama_alice_a01ff09b:

    # mf "Oh no. Look away sir!"
    mf "哦,不.把脸转开,先生!"

